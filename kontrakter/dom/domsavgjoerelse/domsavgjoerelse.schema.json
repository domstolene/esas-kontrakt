      "minimum": 1,
      "maximum": 99
    },
    "dager": {
      "type": "integer",
      "minimum": 1,
      "maximum": 999
    },
    "timer": {
      "type": "integer",
      "minimum": 1,
      "maximum": 999
    },
    "GUID": {
      "type": "string",
      "pattern": "^[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}$"
