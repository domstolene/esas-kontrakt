          "pattern": "^[0-7][0-9][012389][0-9]+$",
          "minLength": 11,
          "maxLength": 11
        }
      },
      "required": ["idType", "verdi"],
      "additionalProperties": false
    },
    "saervilkaar": {
      "properties": {
